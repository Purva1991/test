package assignment1;

public class fibo {

	public static void main(String[] args) {
		int n = 50, prev = 0, next = 1, sum = 0;
		System.out.println("Fibonacci Series:");
		System.out.println(prev);
		System.out.println(next);
		while (sum < n) {
			sum = prev + next;
			prev = next;
			next = sum;hbdahgJK
			if (sum < n) {
				System.out.println(sum);
			}
		}

	}

}
